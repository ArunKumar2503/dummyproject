{
  "name": "call_service",
  "version": "1.0.0",
  "description": "This is call service http rest service",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --watch './src/**/*.ts' --exec ts-node ./src/index.ts",
    "build": "tsc",
    "test": "jest --coverage",
    "test:watch": "npm run test -- --watchAll",
    ".": "nodemon",
    "start": "node --inspect=5858 -r ts-node/register ./src/index.ts",
    "socketserver": "node --inspect=5859 -r ts-node/register ./src/socket.ts",
    "start-socket": "node --inspect=5858 -r ts-node/register ./src/socketio-redis/socket-server.ts",
    "start:prod": "node dist/index.js",
    "prebuild": "npm run lint",
    "prestart": "npm run build",
    "postinstall": "npm run build",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix"
